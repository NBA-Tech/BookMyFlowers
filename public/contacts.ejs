                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xxl-12">
                        <div class="card__wrapper">
                            <div class="table__wrapper table-responsive">
                                <table class="table mb-20" id="dataTableDefualt">
                                    <thead>
                                        <tr class="table__title table__sort">

                                            <th>Author ID</th>
                                            <th>Name</th>
                                            <th>Email</th>
                                            <th>Mobile</th>
                                            <th>Date Posted</th>
                                            <th>City</th>
                                            <th>Contact Category</th>
                                            <th>Message</th>
                                        </tr>
                                    </thead>
                                    <tbody class="table__body">
                                        <% harmonised_contact_data.forEach((item)=>{ %>
                                        <tr>
                                            <td>

                                                <%= item?.post_author %>
                                            </td>

                                            <td><%= item?.name %></td>

                                            <td><%= item?.email %></td>

                                            <td><%= item?.mobile %></td>

                                            <td><%= item?.date_posted %></td>

                                            <td><%= item?.city %></td>
                                            <td>

                                            <% if(item?.contact_category == 'Collaborate with Us'){ %>
                                            
                                            <span class="bd-badge bg-success">
                                                <%= item?.contact_category %>
                                            </span>

                                            <% } else if(item?.contact_category == 'Franchise Enquiries'){ %>

                                            <span class="bd-badge bg-secondary">
                                                <%= item?.contact_category %>
                                            </span>

                                            <% } else { %>

                                             <%= item?.contact_category %>

                                            <% } %>
                                        </td>

                                            <td> <%= item?.message %></td>


                                              
                                        </tr>
                                        
                                        <% }) %>
                                    </tbody>
                                </table>
                            </div>
